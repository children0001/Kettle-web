      <bordercolorgreen>100</bordercolorgreen>
      <bordercolorblue>100</bordercolorblue>
      <drawshadow>N</drawshadow>
    </notepad>
    <notepad>
      <note>Any string configuration parameter of a VFS scheme is supported by default.</note>
      <xloc>10</xloc>
      <yloc>410</yloc>
      <width>585</width>
      <heigth>29</heigth>
      <fontname/>
